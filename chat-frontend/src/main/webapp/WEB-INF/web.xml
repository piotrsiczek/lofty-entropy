<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <servlet>
        <servlet-name>forntendService</servlet-name>
        <servlet-class>com.spiczek.chat.backend.service.forntendServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>forntendService</servlet-name>
        <url-pattern>/app/forntendService</url-pattern>
    </servlet-mapping>

    <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>app.html</welcome-file>
    </welcome-file-list>

</web-app>
