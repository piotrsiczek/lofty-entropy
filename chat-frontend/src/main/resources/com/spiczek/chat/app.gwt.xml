<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="app">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.http.HTTP"/>

    <inherits name="com.google.gwt.uibinder.UiBinder"/>
    <inherits name='com.google.web.bindery.event.EventBinder'/>
    <!--<inherits name='com.google.gwt.user.theme.standard.Standard'/>-->

    <inherits name="com.spiczek.chat.backend"/>
    <inherits name="com.spiczek.chat.shared"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.spiczek.chat.frontend.app'/>

    <source path='frontend'/>


    <!--&lt;!&ndash; Specify the app servlets.                   &ndash;&gt;-->
    <!--<servlet path='/clientService' class='com.spiczek.chat.client.server.clientServiceImpl'/>-->
    <!--<servlet path="/TokenService" class="com.spiczek.chat.client.server.TokenServiceImpl"/>-->
    <!--<servlet path="/MessageService" class="com.spiczek.chat.client.server.MessageServiceImpl"/>-->

</module>